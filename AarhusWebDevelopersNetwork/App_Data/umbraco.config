      <projectItem id="1113" key="2ad9e349-214a-482d-a15d-c3d41c1b08f8" parentID="1090" level="3" creatorID="0" sortOrder="0" createDate="2016-10-03T10:57:42" updateDate="2016-10-03T10:58:55" nodeName="Online Mobile Recharge Portal Project" urlName="online-mobile-recharge-portal-project" path="-1,1082,1090,1113" isDoc="" nodeType="1069" creatorName="Jakob Bak" writerName="Jakob Bak" writerID="0" template="1068" nodeTypeAlias="projectItem">
