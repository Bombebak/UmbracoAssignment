      <projectItem id="1119" key="5fae1acf-7ccf-487f-8390-72f76224de08" parentID="1090" level="3" creatorID="0" sortOrder="2" createDate="2016-10-10T09:44:41" updateDate="2016-10-10T09:44:41" nodeName="Image Steganography With 3 Way Encryption" urlName="image-steganography-with-3-way-encryption" path="-1,1082,1090,1119" isDoc="" nodeType="1069" creatorName="Jakob Bak" writerName="Jakob Bak" writerID="0" template="1068" nodeTypeAlias="projectItem">
